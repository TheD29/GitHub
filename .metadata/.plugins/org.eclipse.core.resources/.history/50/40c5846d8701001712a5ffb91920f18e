package TestProject;

public class Avto {
	static Avto2[] autos = new Avto2[] { new Car(4, "Car"), new miniBuss(8, "miniBuss") };

	public static void main(String[] args) {
		int people = 10;
		/*
		 * Avto2 car = new Car(4, "CAR"); miniBuss miniBuss = new miniBuss(8,
		 * "MiniBuss"); Buss buss = new Buss(12, "Buss");
		 */

		if (car.getPlace() >= people)
			System.out.println(car.getName());
		else if (miniBuss.getPlace() >= people)
			System.out.println(miniBuss.getName());
		else if (buss.getPlace() >= people)
			System.out.println(buss.getName());

	}

}
