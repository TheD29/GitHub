		int indexMin = 0;
		for (int i = 0; i < autos.length; i++) {
			model model = autos[i];
			for (int j = 0; j < autos.length; j++) {
				auto = autos[i];
				indexMin = j;

			}
			sum += autos[i].getSum();
			System.out.println(autos[i].getFlower() + ' ' + autos[i].getSum());
