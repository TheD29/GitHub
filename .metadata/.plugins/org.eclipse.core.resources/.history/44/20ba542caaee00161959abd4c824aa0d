package test;

public class arraay_operation2 {

	public static void main(String[] args) {
		// операції з масивами (копіювання, багатовимірні масиви)
		int[] arrNum = { 1, 3, 25, -5, 50, 2, 78, 29, 15 };
		int[] arrNum2 = { 3, 5, 15, 20, -3, 22, 150, 60, 80 };
		int arrNull[] = new int[8];
		int arrS1 = 0;
		int arrS2 = 0;
		for (int i : arrNum) {
			arrS1 = arrNum[i];
		
		for (int j : arrNum2) {
			arrS2 = arrNum2[j];
			arrNull[i] = arrS1 + arrS2;
			System.out.println(arrS1);
		}
		}
	}

}