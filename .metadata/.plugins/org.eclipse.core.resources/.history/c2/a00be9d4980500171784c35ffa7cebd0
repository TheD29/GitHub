package TestProject;

public class Controller extends Auto {
	Auto auto;
	mainAuto mn;

	public Controller() {

	}

	public Controller(int place, String name) {
		super(place, name);
		// TODO Auto-generated constructor stub
	}

	public void number() {
		for (int i = 0; i < mn.autos.length; i++) {
			if (auto.getPeople() <= mn.autos[i].length) {
				System.out.println(mn.autos[i].getPlace());
			}
		}

	}
}
