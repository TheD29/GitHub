package L4;

public class ArrayDemensionLeft {

	public static void main(String[] args) {
		int n = 2;
		int array[][] = { { 2, 4, 5, 3 }, { 3, 8, 10, 1 }, { 7, 9, 6, 32 }, { 22, 31, 50, 60 } };
		int val = 0;
		int val2 = 0;
		for (int i = 0; i < n; ++i) {
			val = array[0][array.length-1];
			array[0][array.length-1] = array[0][0];
			
			//System.out.println(val);
			//System.out.println(array[0][array.length-1]);
			for (int j = 0; j < array.length-1; ++j) {
				val2 = array[0][j+1];
				array[0][j] = val2;
				//array[0][array.length-1] = val;
				//val2 = val;
				//System.out.print(array[0][j+1]);
				System.out.print(array[i][j]);
			}
		}
		
		for (int i = 0; i < array.length; i++) {
			for (int j = 0; j < array.length; j++) {
				//array[0][array.length-1] = val;
				System.out.print(array[i][j]+"\t");
			}
			System.out.println();
		}
		
	}

}
