package Object;

public class Animal {
	private final String name;// Ключове слово final означає що значення змінної
								// не можна змінити після її ініціалізації
	private int age;

	public Animal(String name, int age) {
		super();
		this.name = name;
		this.age = age;
	}

	public int getAge() {
		return age;
	}

	public String getName() {
		return name;
	}

	public void setName() {
		final String s;
		s = "ssssss";
		// s = "asdas"; не можна переініцілізувати змінну
	}
}
