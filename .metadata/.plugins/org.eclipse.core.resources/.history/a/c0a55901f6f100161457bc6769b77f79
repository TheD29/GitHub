package test;

public class twoDemancionsArray {

	public static void main(String[] args) {
		int n = 5;
		int len = 1;
		int array[][] = new int[n][n];
		int mas[][] = new int[n][n];
		for (int i = 0; i < array.length; i++) {
			for (int j = 0; j < array.length; ++j) {
				array[i][j] = (i*j+1)+(i*j+1);
				// array[i] = array[i + 1];

				System.out.print(array[i][j] + " ");
				//System.out.println(mas[i]);
			}
			for (int j = 0; j < len; ++j) {
				int val = array[1][len];
				array[1][len]= array[1][array.length-1];
				for (int k = 0; k < mas.length-1; ++k) {
					int val2 = mas[1][j+1];
					mas[1][j+1] = val;
					val = val2;
					System.out.println(array[i][j]);
				}
			}
			System.out.println();
		}

	}

}
