package L4;

public class DemensionArray2 {

	public static void main(String[] args) {
		int n = 1;
		int array[][] = { { 2, 4, 5, 3 }, { 3, 8, 10, 1 }, { 7, 9, 6, 32 }, { 22, 31, 50, 60 } };
		for (int i = 0; i < array.length; i++) {
			for (int j = 0; j < array.length; j++) {
				System.out.print(array[i][j] + "\t");
			}
			System.out.println();
		}
	}

}
