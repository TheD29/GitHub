package Flowers;

public class main {
	static model[] m = new model[] { new Rose("Rose", 1, 15.2), new BlueRose("BlueRose", 3, 16),
			new WhiteRose("WhiteRose", 10, 11.3) };

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		double sum = 0;
		for (int i = 0; i < m.length; i++) {
			
			m[i].setBouquet(m[i].getSum());
			sum += m[i].getBouquet();
			System.out.println(m[i].getFlower() + ' ' + m[i].getSum());
			System.out.println(sum);

			//System.out.println(m[i].getBouquet());

		}

	}

}
