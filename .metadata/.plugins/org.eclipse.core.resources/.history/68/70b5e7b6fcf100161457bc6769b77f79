package test;

public class twoDemancionsArray {

	public static void main(String[] args) {
		int chng = 2;// на скільки наш масив буде зміщуватись
		int array[] = { 1, 2, 45, 69, 18 };
		int var = 0;
		int var2 = 0;
		for (int i = 0; i < chng; i++) { //запам"ятовуємо значення масива
			var = array[0];			//перше значення
			array[0] = array[array.length - 1];//міняєм пперший елемент масиву на останній
			// System.out.print(array[i] + "\t");
			for (int j = 0; j < array.length - 1; ++j) {
		//		System.out.println(array[j]);
				var2 = array[j + 1];
				array[j + 1] = var;
				var = var2;

			}

		}

	/*	for (int i = 0; i < array.length; i++) {
			System.out.println(array[i]);
		}*/

		System.out.println();

	}

}
